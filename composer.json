{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "consoletvs/charts": "6.*",
        "cyrildewit/eloquent-viewable": "^2.4",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "karlmonson/laravel-ping": "^1.0",
        "laravel-notification-channels/telegram": "^1.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "laravolt/indonesia": "^0.7.0",
        "league/flysystem-sftp": "^1.0",
        "maatwebsite/excel": "^3.1",
        "orchestra/parser": "3.7.*",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.7",
        "rodenastyle/stream-parser": "^1.2",
        "spatie/browsershot": "^3.29",
        "spatie/laravel-backup": "^5.11",
        "spatie/laravel-cors": "^1.6",
        "spatie/laravel-permission": "^2.7",
        "stevebauman/location": "^6.1",
        "symfony/psr-http-message-bridge": "^1.1",
        "tymon/jwt-auth": "dev-develop",
        "watson/active": "^2.0",
        "zendframework/zend-diactoros": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
